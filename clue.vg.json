{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "title": "Test Map",
    "width": 1200,
    "height": 800,
    "projection": {
        "type": "mercator",
        "pointRadius": 1
    },
    "layer": [
        {
            "data": {
                "url": "/data/MB_2016_VIC.json",
                "format": {"type": "topojson", "feature": "MB_2016_VIC"}
            },
            "mark": {"type": "geoshape", "fill": "lightgrey", "stroke": "white"}
        },
        {
            "data": {
                "url": "/data/clue-offstreet-car-parking-2017.json",
                "format": {"type": "json", "property": "features"}
            },
            "mark": {"type": "geoshape", "color": "green"}
        },
        {
            "data": {
                "url": "/data/on-street-parking-bay-sensors.geojson",
                "format": {"type": "json", "property": "features"}
            },
            "mark": {"type": "geoshape", "color": "red"}
        }
    ]
}
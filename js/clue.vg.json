{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 500,
    "height": 500,
    "projection": {
        "type": "mercator",
        "pointRadius": 1
    },
    "layer": [
        {
            "data": {
                "url": "../data/MB_2016_VIC.json",
                "format": {"type": "topojson", "feature": "MB_2016_VIC"}
            },
            "mark": {"type": "geoshape", "fill": "lightgrey", "stroke": "white"}

        },
        {
            "data": {
                "url": "../data/on-street-parking-bay-sensors.json",
                "format": {"type": "topojson", "feature": "on-street-parking-bay-sensors"}
            },
            "mark": {"type": "geoshape", "color": "red"}
        },
        {
            "data": {
                "url": "../data/clue-offstreet-car-parking-2017-crop.json",
                "format": {"type": "topojson", "feature": "clue-offstreet-car-parking-2017"}
            },
            "mark": {"type": "geoshape", "color": "green"}
        }
    ]
}